<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="activity.project.uasirvanapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.irvanchandra">
        <activity android:name="activity.project.uasirvanapp.ScanActivity" />
        <activity android:name="activity.project.uasirvanapp.LoginActivity" />
        <activity android:name="activity.project.uasirvanapp.getStarted" />
        <activity android:name="activity.project.uasirvanapp.DaftarActivity" />
        <activity android:name="activity.project.uasirvanapp.DataActivity" />
        <activity android:name="activity.project.uasirvanapp.MainActivity" />
        <activity
            android:name="activity.project.uasirvanapp.SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>